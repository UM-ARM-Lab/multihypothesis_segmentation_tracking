<launch>
    <arg name="gui" default="false"/>
   	<arg name="world" default="victor_table"/>

    <!-- Startup simulation world -->
    <include file="$(find mps_interactive_segmentation)/test/system/test_domain.launch">
        <arg name="world" value="$(arg world)"/>
        <arg name="gui" value="$(arg gui)"/>
    </include>

    <node pkg="mps_interactive_segmentation" type="gazebo_segmentation_gt" name="gazebo_segmentation_gt">
    	<remap from="gazebo_segmentation_gt/gazebo_segmentation" to="/segment_rgbd"/>
    </node>

    <param name="/use_sim_time" value="true"/>
    <node pkg="mps_test" type="generate_gazebo_benchmark" name="generate_gazebo_benchmark" required="true">
    </node>

</launch>